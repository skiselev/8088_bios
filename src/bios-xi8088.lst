   400                              <1> ; set_system_timer - set timer variables to RTC time
   401                              <1> ;-------------------------------------------------------------------------
   402                              <1> set_system_timer:
   403 00000CF9 50                  <1> 	push	ax
   404 00000CFA 53                  <1> 	push	bx
   405 00000CFB 51                  <1> 	push	cx
   406 00000CFC 52                  <1> 	push	dx
   407 00000CFD 56                  <1> 	push	si
   408 00000CFE 57                  <1> 	push	di
   409                              <1> 
   410 00000CFF B402                <1> 	mov	ah,02h			; int 1Ah, function 02h - get RTC time
   411 00000D01 CD1A                <1> 	int	1Ah
   412                              <1> 
   413                              <1> ; convert time to ticks * 2^11
   414                              <1> 
   415                              <1> ; ticks = seconds * 37287
   416 00000D03 88F0                <1> 	mov	al,dh
   417 00000D05 E80301              <1> 	call	bcd_to_binary		; convert seconds to binary
   418                              <1> 
   419 00000D08 BAA791              <1> 	mov	dx,37287
   420 00000D0B F7E2                <1> 	mul	dx			; DX:AX = seconds * 37287
   421                              <1> 
   422 00000D0D 89C6                <1> 	mov	si,ax
   423 00000D0F 89D7                <1> 	mov	di,dx
   424                              <1> 
   425                              <1> ; ticks += minutes * 2237216 = minutes * 8992 + minutes * 34 * 2^16
   426 00000D11 88C8                <1> 	mov	al,cl
   427 00000D13 E8F500              <1> 	call	bcd_to_binary		; convert minutes to binary
   428                              <1> 
   429 00000D16 89C3                <1> 	mov	bx,ax
   430 00000D18 BA2023              <1> 	mov	dx,8992
   431 00000D1B F7E2                <1> 	mul	dx			; DX:AX = minutes * 8992
   432                              <1> 
   433 00000D1D 01C6                <1> 	add	si,ax
   434 00000D1F 11D7                <1> 	adc	di,dx
   435                              <1> 
   436 00000D21 89D8                <1> 	mov	ax,bx
   437 00000D23 BA2200              <1> 	mov	dx,34
   438 00000D26 F7E2                <1> 	mul	dx
   439                              <1> 
   440 00000D28 01C7                <1> 	add	di,ax
   441                              <1> 
   442                              <1> ; ticks += hours * 134232938 = hours * 15210 + hours * 2048 * 2^16
   443 00000D2A 88E8                <1> 	mov	al,ch
   444 00000D2C E8DC00              <1> 	call	bcd_to_binary		; convert hours to binary
   445                              <1> 
   446 00000D2F 89C3                <1> 	mov	bx,ax
   447 00000D31 BA6A3B              <1> 	mov	dx,15210
   448 00000D34 F7E2                <1> 	mul	dx			; DX:AX = hours * 15210
   449                              <1> 
   450 00000D36 01C6                <1> 	add	si,ax
   451 00000D38 11D7                <1> 	adc	di,dx
   452                              <1> 
   453 00000D3A 89D8                <1> 	mov	ax,bx
   454 00000D3C BA0008              <1> 	mov	dx,2048
   455 00000D3F F7E2                <1> 	mul	dx			; AX = hours * 2048
   456                              <1> 
   457 00000D41 01C7                <1> 	add	di,ax
   458                              <1> 
   459                              <1> ; CX:DX = DI:SI / 2048
   460 00000D43 B10B                <1> 	mov	cl,11
   461 00000D45 D3EE                <1> 	shr	si,cl
   462 00000D47 89FA                <1> 	mov	dx,di
   463 00000D49 B105                <1> 	mov	cl,5
   464 00000D4B D3E2                <1> 	shl	dx,cl
   465 00000D4D 09F2                <1> 	or	dx,si
   466                              <1> 
   467 00000D4F B10B                <1> 	mov	cl,11
   468 00000D51 D3EF                <1> 	shr	di,cl
   469 00000D53 89F9                <1> 	mov	cx,di
   470                              <1> 
   471                              <1> 					; CX = high word of tick count
   472                              <1> 					; DX = low word of tick count
   473                              <1> 	
   474 00000D55 B401                <1> 	mov	ah,01h			; int 1Ah, function 01h - set time
   475 00000D57 CD1A                <1> 	int	1Ah
   476 00000D59 5F                  <1> 	pop	di
   477 00000D5A 5E                  <1> 	pop	si
   478 00000D5B 5A                  <1> 	pop	dx
   479 00000D5C 59                  <1> 	pop	cx
   480 00000D5D 5B                  <1> 	pop	bx
   481 00000D5E 58                  <1> 	pop	ax
   482 00000D5F C3                  <1> 	ret
