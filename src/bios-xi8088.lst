   332                              <1> ;=========================================================================
   333                              <1> ; delay_15us - delay for multiplies of 15 microseconds using PIT
   334                              <1> ; Input:
   335                              <1> ;	CX = time to delay (in 15 microsecond units)
   336                              <1> ;
   337                              <1> ; - Calculate the total number of PIT ticks necessary
   338                              <1> ;	1,193,182 / 1000 ms / 1000 us / * 15 us * 2 = ~ 36 ticks/us
   339                              <1> ; - Latch the PIT and draw down the countdown total on each read.
   340                              <1> ; - Exit when countdown underflows.
   341                              <1> ;
   342                              <1> ; Note: Mode 3 (Square Wave) decrements the readable counter by 2, so the
   343                              <1> ; effective frequency of the counter is actually 2,386,360 Hz.
   344                              <1> ;
   345                              <1> ; Contributed by @640-KB (under GPL-3.0 license)
   346                              <1> ; Based on contribution by @Raffzahn (under CC BY-SA 4.0):
   347                              <1> ; https://retrocomputing.stackexchange.com/a/24874/21323
   348                              <1> ;-------------------------------------------------------------------------
   349                              <1> delay_15us:
   350                              <1> 	push	ax
   351                              <1> 	push	bx
   352                              <1> 	push	cx
   353                              <1> 	push	dx
   354                              <1> 	mov	ax,2*15*pic_freq/1000/1000+1	; ~ 36 ticks/us	
   355                              <1> 	mul	cx		; dx:ax = countdown of pit ticks to wait
   356                              <1> 	xchg	ax,bx		; dx:bx = countdown ticks
   357                              <1> 	call	io_wait_latch	; ax = start read
   358                              <1> 	mov	cx,ax		; cx = last read
   359                              <1> .tick_loop:
   360                              <1> 	call	io_wait_latch	; ax = current counter reading
   361                              <1> 	sub	cx,ax		; cx = # of ticks elapsed since last reading
   362                              <1> 	sub	bx,cx		; subtract change in ticks from countdown
   363                              <1> 	mov	cx,ax		; cx = save the last read
   364                              <1> 	sbb	dx,0		; borrow out of high word (if necessary)
   365                              <1> 	jae	.tick_loop	; loop while countdown >= 0
   366                              <1> 	pop	dx
   367                              <1> 	pop	cx
   368                              <1> 	pop	bx
   369                              <1> 	pop	ax
   370                              <1> 	ret
   371                              <1> 
   372                              <1> ;=========================================================================
   373                              <1> ; Latch PIT 0 and read counter
   374                              <1> ; Output:
   375                              <1> ;	AX = current counter
   376                              <1> ;-------------------------------------------------------------------------
   377                              <1> io_wait_latch:
   378                              <1> 	mov	al,0		; counter 0, latch (00b)
   379                              <1> 	pushf			; save current IF
   380                              <1> 	cli			; disable interrupts
   381                              <1> 	out	pit_ctl_reg,al	; write command to ctc
   382                              <1> 	in	al,pit_ch0_reg	; read low byte of counter 0 latch
   383                              <1> 	mov	ah,al		; save it
   384                              <1> 	in	al,pit_ch0_reg	; read high byte of counter 0 latch
   385                              <1> 	popf			; restore IF state
   386                              <1> 	xchg	al,ah		; convert endian
   387                              <1> 	ret
   388                              <1> 
   389                              <1> %else ; LOOP_DELAY
   390                              <1> 
   391                              <1> ;=========================================================================
   392                              <1> ; delay_15us - delay for multiplies of approximately 15 microseconds
   393                              <1> ; Input:
   394                              <1> ;	CX = time to delay (in 15 microsecond units)
   395                              <1> ; Notes:
   396                              <1> ;	This implementation does not provide precise timing
   397                              <1> ;	The actual delay depends on the CPU clock frequency
   398                              <1> ;-------------------------------------------------------------------------
   399                              <1> delay_15us:
   400                              <1> 	push	ax
   401                              <1> 	push	cx
   402                              <1> .1:
   403                              <1> 	mov	al,4
   404                              <1> .2:
   405                              <1> 	dec	al
   406                              <1> 	jnz	.2
   407                              <1> 	loop	.1
   408                              <1> 	pop	cx
   409                              <1> 	pop	ax
   410                              <1> 	ret
   411                              <1> 
