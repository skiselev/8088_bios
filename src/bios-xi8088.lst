   844                              <1> 	; 60h-63h are aliased to 70h-73h, so RTC cannot use addresses 70h-71h.
   845                              <1>         ; Therefore port 62h (ppi_pc_reg) reads the same as port 72h.
   846                              <1> 	in	al,ppi_pc_reg
   847                              <1> 	mov	ah,al
   848                              <1> 	in	al,72h
   849                              <1> 	cmp	al,ah
   850                              <1> 	jne	.not_fe2010
   851                              <1> 	stc				; FE2010, will not try to detect RTC
   852                              <1> 	jmp	.exit
   853                              <1> .not_fe2010:
