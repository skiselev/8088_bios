   523                              <1> .sub_done:
   524                              <1> 	pop	ax	; pull the count from the simple run
   525                              <1> 	sub	bx,ax
   526                              <1> 	; We'd expect BX to be the amount of clock ticks corresponding to (some large number) more cycles
   527                              <1> 
