   331                              <1> ;=========================================================================
   332                              <1> ; rtc_read - Read byte from RTC or CMOS memory
   333                              <1> ; Input:
   334                              <1> ;	AL - address and NMI enable bit
   335                              <1> ;		bits 6-0 - address of byte to read
   336                              <1> ;		bit 7    - 0 = disable NMI, 1 = enable NMI
   337                              <1> ; Output:
   338                              <1> ;	AL = byte from RTC
   339                              <1> ;-------------------------------------------------------------------------
   340                              <1> rtc_read:
   341 00000CD7 FA                  <1> 	cli
   342                              <1> %ifndef AT_RTC_PORT
   343 00000CD8 E670                <1> 	out	rtc_addr_reg,al
   344 00000CDA EB00                <1> 	jmp	$+2
   345 00000CDC EB00                <1> 	jmp	$+2
   346 00000CDE EB00                <1> 	jmp	$+2
   347 00000CE0 EB00                <1> 	jmp	$+2
   348 00000CE2 E471                <1> 	in	al,rtc_data_reg
   349                              <1> %else ; AT_RTC_PORT
   350                              <1> 	push	dx
   351                              <1> 	mov	dx,AT_RTC_PORT
   352                              <1> 	out	dx,al
   353                              <1> 	jmp	$+2
   354                              <1> 	jmp	$+2
   355                              <1> 	jmp	$+2
   356                              <1> 	jmp	$+2
   357                              <1> 	inc	dx			; DX = RTC data register
   358                              <1> 	in	al,dx
   359                              <1> 	pop	dx
   360                              <1> %endif ; AT_RTC_PORT
   361 00000CE4 FB                  <1> 	sti
   362 00000CE5 C3                  <1> 	ret
   363                              <1> 
   364                              <1> ;=========================================================================
   365                              <1> ; rtc_write - Read byte to RTC or CMOS memory
   366                              <1> ; Input:
   367                              <1> ;	AL - address and NMI enable bit
   368                              <1> ;		bits 6-0 - address of byte to read
   369                              <1> ;		bit 7    - 0 = disable NMI, 1 = enable NMI
   370                              <1> ;	AH = byte to write to RTC
   371                              <1> ;-------------------------------------------------------------------------
   372                              <1> rtc_write:
   373 00000CE6 FA                  <1> 	cli
   374                              <1> %ifndef AT_RTC_PORT
   375 00000CE7 E670                <1> 	out	rtc_addr_reg,al
   376 00000CE9 EB00                <1> 	jmp	$+2
   377 00000CEB EB00                <1> 	jmp	$+2
   378 00000CED EB00                <1> 	jmp	$+2
   379 00000CEF EB00                <1> 	jmp	$+2
   380 00000CF1 86E0                <1> 	xchg	ah,al
   381 00000CF3 E671                <1> 	out	rtc_data_reg,al
   382                              <1> %else ; AT_RTC_PORT
   383                              <1> 	push	dx
   384                              <1> 	mov	dx,AT_RTC_PORT
   385                              <1> 	out	dx,al
   386                              <1> 	jmp	$+2
   387                              <1> 	jmp	$+2
   388                              <1> 	jmp	$+2
   389                              <1> 	jmp	$+2
   390                              <1> 	xchg	ah,al
   391                              <1> 	inc	dx			; DX = RTC data register
   392                              <1> 	out	dx,al
   393                              <1> 	pop	dx
   394                              <1> %endif ; AT_RTC_PORT
   395 00000CF5 86E0                <1> 	xchg	ah,al
   396 00000CF7 FB                  <1> 	sti
   397 00000CF8 C3                  <1> 	ret
