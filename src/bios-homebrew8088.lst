   518                              <1> ;-------------------------------------------------------------------------
   519                              <1> int_13_fn01:
   520 000008CA 8A264100            <1> 	mov	ah,byte [fdc_last_error]
   521 000008CE 08E4                <1> 	or	ah,ah
   522 000008D0 7401                <1> 	jz	.no_error
   523 000008D2 F9                  <1> 	stc
