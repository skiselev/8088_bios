   426                              <1> ;=========================================================================
   427                              <1> ; fdc_init - Initialize floppy disk controller
   428                              <1> ; Input:
   429                              <1> ;	none
   430                              <1> ; Output:
   431                              <1> ;	CF clear on success
   432                              <1> ;		AH = 00h - successful completion
   433                              <1> ;	CF set if error
   434                              <1> ;		AH = 20h - controller failure
   435                              <1> ;		AH = 80h - timeout / device not ready
   436                              <1> ;-------------------------------------------------------------------------
   437                              <1> fdc_init:
   438 00000865 80263E00F0          <1> 	and	byte [fdc_calib_state],0F0h ; require recalibration
   439 0000086A E8BA05              <1> 	call	fdc_reset		; reset FDC
   440 0000086D 7257                <1> 	jc	fdc_init_error
   441 0000086F BAF403              <1> 	mov	dx,fdc_status_reg	; get status register address
   442 00000872 EC                  <1> 	in	al,dx
   443 00000873 A880                <1> 	test	al,80h
   444 00000875 7404                <1> 	jz	.try_again		; try again if FDC not ready
   445 00000877 A840                <1> 	test	al,40h
   446 00000879 740F                <1> 	jz	.fdc_ready		; FDC ready to recieve data
   447                              <1> .try_again:
   448 0000087B E8A905              <1> 	call	fdc_reset
   449 0000087E BAF403              <1> 	mov	dx,fdc_status_reg	; get status register address
   450 00000881 EC                  <1> 	in	al,dx
   451 00000882 A880                <1> 	test	al,80h
   452 00000884 7440                <1> 	jz	fdc_init_error
   453 00000886 A840                <1> 	test	al,40h
   454 00000888 753C                <1> 	jnz	fdc_init_error
   455                              <1> .fdc_ready:
   456 0000088A B008                <1> 	mov	al,08h			; FDC Sense Interrupt Status command
   457 0000088C E8D904              <1> 	call	fdc_write		; send the command
   458 0000088F 7235                <1> 	jc	fdc_init_error
   459 00000891 E83105              <1> 	call	fdc_read		; read ST0
   460 00000894 7230                <1> 	jc	fdc_init_error
   461 00000896 A24200              <1> 	mov	byte [fdc_ctrl_status],al  ; save ST0
   462 00000899 88C4                <1> 	mov	ah,al			; save AL to AH
   463 0000089B E82705              <1> 	call	fdc_read		; read current cylinder
   464 0000089E 7226                <1> 	jc	fdc_init_error
   465 000008A0 A24300              <1> 	mov	byte [fdc_ctrl_status+1],al  ; save
   466 000008A3 80E4C0              <1> 	and	ah,0C0h
   467 000008A6 80FCC0              <1> 	cmp	ah,0C0h			; abnormal termination?
   468 000008A9 751B                <1> 	jne	fdc_init_error	; not an abnormal termination
   469                              <1> 
