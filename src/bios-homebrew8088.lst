   534                              <1> ; kbc_aux_send - send command to auxiliary device, wait for acknowledge
   535                              <1> ; Input:
   536                              <1> ;	AL = command
   537                              <1> ; Output:
   538                              <1> ;	AH - status:
   539                              <1> ;		00h - success
   540                              <1> ;		03h - interface error (timeout)
   541                              <1> ;		04h - resend requested
   542                              <1> ;	CF == 0 - no error
   543                              <1> ;	CF == 1 - error
   544                              <1> ;-------------------------------------------------------------------------
   545                              <1> kbc_aux_send:
   546                              <1> 	push	cx
