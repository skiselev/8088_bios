   471                              <1> 
   472                              <1> ;=========================================================================
   473                              <1> ; fdc_send_specify - Send specify command to FDC
   474                              <1> ; Input:
   475                              <1> ;	none
   476                              <1> ; Output:
   477                              <1> ;	CF clear on success
   478                              <1> ;		AH = 00 - successful completion
   479                              <1> ;	CF set if error
   480                              <1> ;		AH = 20h - controller failure
   481                              <1> ;	AX,CL,SI trashed
   482                              <1> ; Note:
   483                              <1> ;	Specify command parameters are obtained from table at INT 1Eh vector
   484                              <1> ;-------------------------------------------------------------------------
   485                              <1> fdc_send_specify:
   486 000008AB 1E                  <1> 	push	ds
   487 000008AC 31F6                <1> 	xor	si,si
   488 000008AE 8EDE                <1> 	mov	ds,si
   489 000008B0 C5367800            <1> 	lds	si,[1Eh*4]		; DS:SI -> INT 1Eh
   490 000008B4 B003                <1> 	mov	al,3		 	; FDC Specify command
   491 000008B6 8A24                <1> 	mov	ah,byte [si]		; specify command - byte 0
   492 000008B8 8B7401              <1> 	mov	si,word [si+1]		; specify command - byte 1
   493 000008BB B103                <1> 	mov	cl,3			; command length 3 bytes (AX, SI - low)
   494 000008BD 1F                  <1> 	pop	ds
   495 000008BE E85B04              <1> 	call	fdc_send_cmd		; send specify command to FDC
   496 000008C1 7203                <1> 	jc	fdc_init_error
   497 000008C3 B400                <1> 	mov	ah,fdc_e_success	; successful completion
   498 000008C5 C3                  <1> 	ret
   499                              <1> 
   500                              <1> fdc_init_error:
   501 000008C6 B420                <1> 	mov	ah,fdc_e_failure
   502 000008C8 F9                  <1> 	stc
   503 000008C9 C3                  <1> 	ret
