   558                              <1> ;	cmp	al,dev_rsp_error
   559                              <1> ;	je	.timeout		; treat as timeout/interface error
   560                              <1> 	jmp	.timeout
   561                              <1> 
   562                              <1> .ok:
   563                              <1> 	mov	al,00h			; success - ACK received
   564                              <1> 	clc
   565                              <1> 	jmp	.exit
   566                              <1> 
   567                              <1> .timeout:
   568                              <1> 	mov	al,03h			; interface error
   569                              <1> 	jmp	.error
   570                              <1> 
   571                              <1> .resend:
   572                              <1> 	mov	al,04h			; resend
   573                              <1> 
   574                              <1> .error:
   575                              <1> 	stc
   576                              <1> 
   577                              <1> .exit:
   578                              <1> 	xchg	ah,al			; status to AH, original command to AL
   579                              <1> 	pop	cx
   580                              <1> 	ret
