   548                              <1> 	mov	al,kbc_cmd_aux_snd	; write byte to auxiliary device
   549                              <1> 	call	kbc_send_cmd
   550                              <1> 	jc	.timeout		; kbc_send_cmd timed out
   551                              <1> 	call	kbc_aux_read		; read acknowledge from device
   552                              <1> 	jc	.timeout		; no reply - timeout
   553                              <1> 	cmp	al,dev_rsp_ack
   554                              <1> 	je	.ok
   555                              <1> 	cmp	al,dev_rsp_resend
   556                              <1> 	je	.resend
