   920 000011C7 E680                <1> 	out	post_reg,al
   921 000011C9 B90080              <1> 	mov	cx,8000h		; wait approximately 0.5 seconds
   922 000011CC E838F6              <1> 	call	delay_15us
   923 000011CF 88D0                <1> 	mov	al,dl			; show DL next (minor error number)
   924 000011D1 E680                <1> 	out	post_reg,al
   925 000011D3 B90080              <1> 	mov	cx,8000h		; wait approximately 0.5 seconds
   926 000011D6 E82EF6              <1> 	call	delay_15us
   927 000011D9 88E0                <1> 	mov	al,ah			; show saved value of AL (data/status)
   928 000011DB E680                <1> 	out	post_reg,al
   929 000011DD B90080              <1> 	mov	cx,8000h		; wait approximately 0.5 seconds
   930 000011E0 E824F6              <1> 	call	delay_15us
   931 000011E3 59                  <1> 	pop	cx
   932 000011E4 E2DE                <1> 	loop	.1
   933 000011E6 C3                  <1> 	ret
